<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action = "/compose" method = "post">
    <div class="form-group">
        <label>Title</label>
        <input type = "text" class = "form-control" name = "postTitle" autocomplete="off">
        <label>Post</label>
        <textarea class = "form-control" name="postBody" cols="40" rows="5"></textarea>
    </div>
    <button type = "submit" class="btn btn-primary" name ="button">Publish</button>
</form>


